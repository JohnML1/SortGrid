<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <Package Version="4">
    <PathDelim Value="\"/>
    <Name Value="laz_sortgrid"/>
    <Type Value="RunAndDesignTime"/>
    <Author Value="John Landmesser"/>
    <CompilerOptions>
      <Version Value="11"/>
      <PathDelim Value="\"/>
